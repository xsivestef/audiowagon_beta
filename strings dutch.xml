<resources>
	<!-- The name of the app -->
	<string name="app_name">AudioWagon</string>
	<!-- The title of the playback queue (a list of tracks to be played back; this is NOT a user-defined playlist). 
	It does not seem to appear in the car -->
	<string name="queue_title">Afspeellijst</string>
	<!-- When clicking on the magnifying glass to enter the search mode there are 3 headings shown depending on the
	found items: Tracks/Artists/Albums. If possible do not translate "track" to sth like "song", the audio file could
	also contain an	audiobook or podcast -->
	<string name="search_group_tracks">Nummers</string>
	<string name="search_group_artists">Artiesten</string>
	<string name="search_group_albums">Albums</string>
	<!-- The "browse tree" is the main view when starting the app -->
	<!-- There are 3 categories at the top the user can pick from to select media items: Tracks/Albums/Artists -->
	<string name="browse_tree_category_tracks">Nummers</string>
	<string name="browse_tree_category_artists">Artiesten</string>
	<string name="browse_tree_category_albums">Albums</string>
	<!-- The tracks category contains a "fake" entry, when pressed, all tracks will be played back in shuffled order -->
	<string name="browse_tree_pseudo_track_shuffle_all">Alles willekeurig afspelen</string>
	<!-- When "Play all" is pressed, all items for the currently shown album or artist will be played back -->
	<string name="browse_tree_pseudo_play_all">Alles afspelen</string>
	<!-- When the metadata inside the audio file does not contain information, the album or artist is shown as
	"Unknown" when browsing -->
	<string name="browse_tree_unknown_album">Onbekend album</string>
	<string name="browse_tree_unknown_artist">Onbekende artiest</string>
	<!-- Albums that are marked in the metadata as compilations will show the artist "Various artists" -->
	<string name="browse_tree_various_artists">Diverse artiesten</string>
	<!-- Some "fake" entries shown in the main browse view e.g. when no USB stick is connected yet -->
	<string name="browse_tree_no_entries_title">Geen audio bestanden</string>
	<string name="browse_tree_no_usb_drive">Plug USB-stick in</string>
	<string name="browse_tree_usb_drive_ejected">USB-stick verwijderd of onleesbaar</string>
	<string name="browse_tree_need_to_agree_legal_title">Legal disclaimer niet geaccepteerd</string>
	<string name="browse_tree_need_to_agree_legal_desc">Check de instellingen</string>
	<!-- We use unicode symbols here to avoid the complex linguistic implications in multiple languages for the
	wordings "artists"/"tracks" together with numbers. No need to translate. -->
	<string name="browse_tree_num_albums_num_tracks" translatable="false">%1$d &#x1F4BF; &#x2B1D; %2$d &#x1D160;</string>
	<!-- These "toasts" are short pop-up messages that might be shown to the user at the top or bottom of the screen -->
	<string name="toast_indexing_started">USB indexering gestart …</string>
	<string name="toast_indexing_completed">USB indexering voltooid</string>
	<!-- If there is no next/previous track in the current playlist that the user can skip forward/backward to -->
	<string name="toast_no_next_track">Geen volgend nummer</string>
	<string name="toast_no_prev_track">Geen voorgaand nummer</string>
	<string name="toast_eject_completed">USB-stick kan veilig verwijderd worden</string>
	<!-- please translate only the "ERROR:" part in front, keep the variable placeholder as-is -->
	<string name="toast_error">FOUT: %1$s</string>
	<!-- We assume "USB" is the same in all languages -->
	<string name="toast_error_USB" translatable="false">USB: %1$s</string>
	<string name="toast_error_USB_IO" translatable="false">I/O</string>
	<!-- Error messages -->
	<!-- The app needs to receive "audio focus" before it can play back any audio. The app will lose "audio focus"
	for example when a navigation route guidance is being read out or when a phone call is ongoing. -->
	<string name="toast_error_audio_focus_denied">Audio focus geweigerd</string>
	<string name="toast_error_USB_init">Verbindingsfout met USB-stick</string>
	<string name="toast_error_no_filesystem">Niet ondersteund bestandsformaat (FAT32 vereist)</string>
	<string name="toast_error_library_creation_fail">Aanmaken verzameling mislukt</string>
	<!-- "state" refers to the state the software is in -->
	<string name="toast_error_invalid_state">Ongeldige status</string>
	<!-- These error strings will be prefixed with the toast_error above, e.g. "ERROR: Timeout" -->
	<string name="toast_error_IO">I/O fout</string>
	<string name="toast_error_malformed_data">Data beschadigd</string>
	<string name="toast_error_timeout">Timeout</string>
	<string name="toast_error_not_supported">Niet ondersteund</string>
	<string name="toast_error_no_USB_device">Geen USB-stick</string>
	<string name="toast_error_unknown">Onbekend probleem</string>
	<!-- Names for the playback actions. These are not shown in the car. -->
	<string name="notif_action_play">Play</string>
	<string name="notif_action_pause">Pauze</string>
	<string name="notif_action_stop">Stop</string>
	<!-- Names for actions to skip to next or previous track. Usually not shown on screen, we use icons -->
	<string name="notif_action_next">Volgende</string>
	<string name="notif_action_prev">Vorige</string>
	<!-- Names and descriptions for the notification channels, these may appear in Android settings. -->
	<string name="notif_channel_name">AudioWagon notificaties</string>
	<string name="notif_channel_desc">AudioWagon notificatiekanaal</string>
	<string name="notif_channel_name_indexing">AudioWagon indexering notificaties</string>
	<string name="notif_channel_desc_indexing">AudioWagon indexering notificatiekanaal</string>
	<!-- Notifications will appear e.g. in Polestar 2 in a pull-down drawer at the top of the screen (indicated by an
	 orange line) -->
	<string name="notif_indexing_title">USB indexering status</string>
	<string name="notif_indexing_text_in_progress">Bezig …</string>
	<!-- This text is also shown as a "fake" media item on the main view when USB indexing is ongoing. The usage of
	the colon should avoid any dependency of the cardinal number on the noun "item" -->
	<string name="notif_indexing_text_in_progress_num_items">Gevonden items: %1$d …</string>
	<string name="notif_indexing_text_completed">Voltooid</string>
	<!-- Names for "special" actions. We use icons, so these are not shown in the car. -->
	<string name="action_shuffle_turn_on">Willekeurig aan</string>
	<string name="action_shuffle_turn_off">Willekeurig uit</string>
	<string name="action_repeat_turn_on">Herhalen aan</string>
	<string name="action_repeat_turn_off">Herhalen uit</string>
	<string name="action_eject">Ontkoppelen</string>
	<!-- The settings screen -->
	<string name="settings_title">Instellingen</string>
	<string name="setting_enable_equalizer_title">Equalizer</string>
	<string name="setting_sound_category_title">Geluid</string>
	<string name="setting_equalizer_preset_title">Equalizer voorkeursinstelling</string>
	<string name="equalizer_preset_less_bass">Minder bass</string>
	<string name="equalizer_preset_more_bass">Meer bass</string>
	<string name="equalizer_preset_less_treble">Minder treble</string>
	<string name="equalizer_preset_more_treble">Meer treble</string>
	<!-- These are equalizer presets intended for the Polestar 2 car. Do not mention the name "Polestar" to avoid
	trademark issues. -->
	<string name="equalizer_preset_P2">P2</string>
	<string name="equalizer_preset_P2_plus">P2+</string>
	<!-- This refers to the shape of the equalizer bands where the bass is increased, the middle is normal, and the
	highs are increased. It is shaped like the letter V -->
	<string name="equalizer_preset_V_shape">V-vorm</string>
	<string name="setting_general_category_title">Algemeen</string>
	<string name="setting_log_to_USB_title">Schrijf log bestanden naar USB-stick</string>
	<!-- The user is adressed directly here. Use a formal form if applicable in your language. -->
	<string name="setting_log_to_USB_desc">Geef deze log bestanden aan de ontwikkelaar wanneer je een bug wilt doorgeven.</string>
	<string name="setting_USB_status_title">USB status</string>
	<string name="setting_USB_status_ok">OK</string>
	<string name="setting_USB_status_not_connected">Niet verbonden</string>
	<string name="setting_USB_status_unknown">Onbekend probleem. Probeer de USB-stick opnieuw in te steken of herstart Android</string>
	<string name="setting_USB_status_connected_no_permission">Verbonden. Geen toestemming</string>
	<string name="setting_USB_status_error">Lees/schrijf fout, probeer de USB-stick opnieuw in te steken of herstel/herformatteer (FAT32)
